generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Answer {
  id          String     @id @default(uuid())
  description String
  active      Boolean    @default(true)
  order       Int
  questions   Question[]
}

model Question {
  id          String   @id @default(uuid())
  description String
  active      Boolean  @default(true)
  order       Int
  answers     Answer[]
}
